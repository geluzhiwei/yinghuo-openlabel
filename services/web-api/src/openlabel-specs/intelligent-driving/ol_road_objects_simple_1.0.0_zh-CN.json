                "children": [
                    {
                        "name": "机动车",
                        "definition": {
                            "description": "由动力装置驱动或者牵引，上道路行驶的供人员乘用或者用于运送物品以及进行工程专项作业的轮式车辆，包括汽车及汽车列车、摩托车、轮式专用机械车、挂车、有轨电车、特型机动车和上道路行驶的拖拉机，不包括虽有动力装置但最大设计车速、整备质量、外廓尺寸等指标符合有关国家标准的残疾人机动轮椅车和电动自行车。",
                            "color": "#000000"
                        },
                        "properties": {
                            "driving_status": {
                                "type": "string",
                                "title": "行驶状态",
                                "description": "车辆的行驶状态",
                                "enum": [
                                    "行驶",
                                    "泊车",
                                    "临停"
                                ],
                                "minLength": 1,
                                "default": "unlabeled",
                                "ui:widget": "RadioWidget"
                            },
                            "vehicle_light": {
                                "type": "array",
                                "uniqueItems": true,
                                "title": "车灯",
                                "description": "车辆的灯光状态",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "左转向灯",
                                        "右转向灯",
                                        "刹车灯",
                                        "危险报警闪光灯"
                                    ],
                                    "enumNames": [
                                        "左转向灯",
                                        "右转向灯",
                                        "刹车灯",
                                        "危险报警闪光灯"
                                    ]
                                  },
                                "default": [],
                                "ui:widget": "CheckboxesWidget"
                            },
                            "door_status": {
                                "type": "array",
                                "title": "车门状态",
                                "description": "车门/后备箱/火车后门开启情况",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "关闭",
                                        "左侧门开",
                                        "右侧门开",
                                        "后门开",
                                        "后备箱开"
                                    ],
                                    "enumNames": [
                                        "关闭",
                                        "左侧门开",
                                        "右侧门开",
                                        "后门开",
                                        "后备箱开"
                                    ]
                                  },
                                "default": [],
                                "uniqueItems": true,
                                "ui:widget": "CheckboxesWidget"
                            },
                            "priority": {
                                "type": "array",
                                "title": "优先级",
                                "description": "车辆的通行优先权、危机程度。危险车辆：化学品，液化气，油罐车。紧急车：救护车，消防车，警车。",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "危险车",
                                        "紧急车"
                                    ],
                                    "enumNames": [
                                        "危险车",
                                        "紧急车"
                                    ]
                                  },
                                "default": [],
                                "uniqueItems": true,
                                "ui:widget": "CheckboxesWidget"
                            }
                        },
                        "children": [
                            {
                                "name": "载客汽车",
                                "definition": {
                                    "description":""
                                },
                                "properties": {},
                                "children": [
                                    {
                                        "name": "大型客车",
                                        "definition": {
                                            "description": "车长大于或等于6000mm或者乘坐人数大于或等于20人的载客汽车"
                                        }
                                    },
                                    {
                                        "name": "中型客车",
                                        "definition": {
                                            "description": "车长小于6000mm且乘坐人数为10~19人的载客汽车"
                                        }
                                    },
                                    {
                                        "name": "小型客车",
                                        "definition": {
                                            "description": "车长小于6000mm且乘坐人数小于或等于9人的载客汽车，但不包括微型载客汽车"
                                        }
                                    },
                                    {
                                        "name": "微型客车",
                                        "definition": {
                                            "description": "车长小于或等于3500mm且内燃机气缸总排量小于或等于 1000 mL （对纯电动汽车为驱动电机总峰值功率小于或等于15kW）的载客汽车"
                                        }
                                    }
                                ]
                            },
                            {
                                "name": "载货汽车",
                                "definition": {
                                    "description":""
                                },
                                "properties": {},
                                "children": [
                                    {
                                        "name": "重型货车",
                                        "definition": {
                                            "description": "总质量大于或等于 12000kg 的载货汽车"
                                        }
                                    },
                                    {
                                        "name": "中型货车",
                                        "definition": {
                                            "description": "车长大于或等于6000mm 的载货汽车，或者总质量大于或等于4500kg且小于 12000kg 的载货汽车；但不包括重型载货汽车和低速货车"
                                        }
                                    },
                                    {
                                        "name": "轻型货车",
                                        "definition": {
                                            "description": "车长小于6000mm 且总质量小于4500kg的载货汽车，但不包括微型载货汽车和低速汽车（三轮汽车和低速货车的总称）"
                                        }
                                    },
                                    {
                                        "name": "微型货车",
                                        "definition": {
                                            "description": "车长小于或等于 3500mm 且总质量小于或等于 1800kg 的载货汽车，但不包括低速汽车"
                                        }
                                    },
                                    {
                                        "name": "三轮汽车",
                                        "definition": {
                                            "description": "以柴油机为动力，最大设计车速小于或等于50km/h，总质量小于或等于2000kg，长小于或等于4600mm，宽小于或等于1600mm，高小于或等于2000mm，具有三个车轮的货车。"
                                        }
                                    }
                                ]
                            },
                            {
                                "name": "专项作业车",
                                "definition": {
                                    "description":"装置有专用设备或器具，在设计和制造上用于工程专项（包括卫生医疗）作业的汽车"
                                },
                                "properties": {},
