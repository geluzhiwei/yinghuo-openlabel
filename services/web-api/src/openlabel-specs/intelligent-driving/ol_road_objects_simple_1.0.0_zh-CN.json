                                        }
                                    },
                                    {
                                        "name": "市政环卫车",
                                        "definition": {
                                            "description": "扫路车、洒水车、吸污车、监测车、安保车等"
                                        }
                                    },
                                    {
                                        "name": "其他作业车",
                                        "definition": {
                                            "description": "采血车、医疗车、体检医疗车、电源车、通信车、电视车等"
                                        }
                                    }
                                ]
                            },
                            {
                                "name": "摩托车",
                                "definition": {
                                    "description":"最大设计车速可大于50km/h"
                                },
                                "properties": {}
                            },
                            {
                                "name": "有轨电车"
                            },
                            {
                                "name": "挂车",
                                "definition": {
                                    "description":"包括微型、轻型、中型、重型"
                                }
                            }
                        ]
